<DuiEngine>
  <font face="宋体" size="12"/>

  <editmenu itemskin="skin_menuitem" iconskin="skin_menuicon" itemheight="26" sepskin="skin_menusplit" iconmargin="4" textmargin="8" bgskin="skin_menubg">
    <item id="1" icon="0">剪切</item>
    <item id="2" icon="1">复制</item>
    <item id="3" icon="2">粘贴</item>
    <item id="4" icon="3">删除</item>
    <sep/>
    <item id="5">全选</item>
  </editmenu>
  
  <string>
    <s id="0">宋体</s>
    <s id="1">1.0</s>
  </string>
  
  <skins>

    <!--must have BEGIN-->
    <imglst name="btncheckbox"         src="IDP_BUTTON_CHECKBOX" subwidth="15"  />
    <imglst name="btnRadio"            src="IDP_BUTTON_RADIOBOX" subwidth="15"  />
    <imglst name="focuscheckbox" src="IDP_BUTTON_CHECKBOX_FOCUS"/>
    <imglst name="focusRadio" src="IDP_BUTTON_RADIOBOX_FOCUS"/>
    <!--must have-->
    <scrollbar name="sb_common" src="IDP_SCROLLBAR" margin="3" hasgripper="0"/>
    <!--must have -->
    <imgframe name="comboboxbtn" src="IDP_COMBO_DROPDOWN" subwidth="16"/>
    <!--  must-have End-->
    
    <imgframe name="dlg_frame" owner="DUI-DEMO"   src="IDP_DIALOG_BG_FRAME" left="5" right="5" top="40" bottom="40"/>

    <imgframe name="skin_flatbutton"            src="IDP_BUTTON_FLATBUTTON" left="3" states="4"/>
    <imgframe name="skin_pushbutton"            src="IDP_BUTTON_PUSHBUTTON" left="3" states="4"/>
    <imgframe name="skin_calendarbutton"            src="IDP_CALENDAR_BUTTON" left="3" states="4"/>

    <imglst name="skin_checkbox"         src="IDP_BUTTON_CHECKBOX2" subwidth="15"  />
    <imglst name="skin_checkbox_focus" src="IDP_BUTTON_CHECKBOX2_FOCUS"/>

    <imglst name="skin_btn_sys_close"    src="IDP_SYSBUTTON_CLOSE" subwidth="36" />
    <imglst name="btn_sys_minimize" src="IDP_SYSBUTTON_MINIMIZE" subwidth="36" />
    <imglst name="btn_sys_maximize" src="IDP_SYSBUTTON_MAXIMIZE" subwidth="36" />
    <imglst name="btn_sys_restore" src="IDP_SYSBUTTON_RESOTRE" subwidth="36" />

    <imglst name="tabskin_default" src="IDP_TAB_COMMON" subwidth="94"/>

    <button name="normalbtn" border="7D9EBC" bg="FBFCFD" bgup="FEFEFE"
        bgdown="C6E2FD" bguphover="FEFEFE" bgdownhover="DBEDFE"
        bguppush="C6E2FD" bgdownpush="FEFEFE"/>
    
    <scrollbar name="sb_test" src="IDP_SCROLLBAR2" margin="3" hasgripper="0" hasinactive="1"/>
    <vscrollbar name="lc_sb_vert" src="IDP_LISTCTRL_SCROLLBAR_VERT" margin="3"/>

    <imgframe name="skin_editnc" src="IDP_EDITBOX_BORDER" subwidth="26" left="2" top="2"/>
    <imgframe name="skin_seperator_vert" src="IDP_SEPERATOR_VERT" top="20" bottom="10"/>
    <imgframe name="skin_seperator_horz" src="IDP_SEPERATOR_HORZ" left="20" right="10"/>
    
    <imglst name="skin_icons_large" src="IDP_ICONS_LARGE" subwidth="32"/>
    <imglst name="skin_menuicon" src="IDP_ICONS_SMALL" subwidth="18"/>
    <imgframe name="skin_menuitem" src="IDP_MENU_SKIN" subwidth="100" left="35" right="5"/>
    <imglst name="skin_menusplit" src="IDP_MENU_SEPERATOR"/>
    <imgframe name="skin_menubg" src="IDP_MENU_BG" left="30" top="5" right="5"/>
    <imglst name="skin_menucheck" src="IDP_MENU_CHECK" subwidth="16"/>
    <border name="skin_menuborder" src="IDP_MENU_BORDER" left="2" top="2" border="2,2,2,2" key="FF00FF" alpha="100"/>
    <imglst name="skin_webbtn_back" src="IDP_WEBBTN_BACK" subwidth="27"/>
    <imglst name="skin_webbtn_forward" src="IDP_WEBBTN_FORWARD" subwidth="27"/>
    <imglst name="skin_webbtn_refresh" src="IDP_WEBBTN_REFRESH" subwidth="27"/>
    <imglst name="bmpmask" src="IDP_BMPMASK">
      <imgparam mask="FF00FF"/>
    </imglst>
    <gif name="face0" src="IDP_DEMO_GIF"/>
    <imglst name="TreeToggle"					src="IDR_TREE_TOGGLE" subwidth="17"/>
    <imglst name="TreeCheckBox"				src="IDR_TREE_CHECKBOX" subwidth="17"/>
    <imglst name="TreeIcon"						src="IDR_TREE_ICON" subwidth="16"/>
    <imglst name="comboIcons"							src="IDP_COMBO_ICONS"  subwidth="15"/>

    <imgframe name="slider_fg" src="IDP_SLIDER_FG" left="3"/>
    <imgframe name="slider_bg" src="IDP_SLIDER_BG" left="5"/>
    <imglst name="slidethumb" src="IDP_SLIDER_THUMB" subwidth="18"/>
    <imgframe name="prog_bar" src="IDP_PROGRESSBAR_FG" left="3"/>
    <imgframe name="prog_bg" src="IDP_PROGRESSBAR_BG" left="5"/>
    <imgframe name="listctrl_header" src="IDP_LISTCTRL_HEADER" left="3" states="4"/>

  </skins>

  <style>
    <class name="cls_dlg_frame" skin="dlg_frame" font="0000" crtext="FFFFFF" x-margin="0"/>

    <class name="cls_btn_link"    cursor="hand" crhover="0A84D2" />
    <!--定义文字按钮的样式-->
    <class name="cls_btn_weblink" cursor="hand" crtext="1e78d5" crhover="1e78d5" hoverfont="0002" />

    <class name="cls_txt_red"     font="0004:宋体"  crtext="FF0000" />
    <!--定义白色粗体宋体-->
    <class name="cls_txt_black"   font="0004:宋体"  crtext="000000" />
    <!--定义黑色粗体宋体-->
    <class name="cls_txt_white"   font="0004:宋体"  crtext="FFFFFF" />
    <!--定义白色粗体宋体-->
    <class name="Arial_txt_white_center"   font="0004:Arial" textmode="25" crtext="FFFFFF" />

    <class name="normalbtn" skin="normalbtn" font="0000" crtext="385e8b" crdisabled="91a7c0" textmode="25" cursor="hand" x-margin="0"/>

    <class name="toptext" textmode="20" />
    <class name="vcentertext" textmode="24"/>
    <class name="rightvcentertext" textmode="26"/>
    <class name="centertext" textmode="25"/>
    <class name="righttext" textmode="22"/>
    <class name="Arial_txt_white"   font="0000:Arial" textmode="25" crtext="FCF5F1" />
    <class name="Arial_txt_gray"   font="0000:Arial" textmode="25" crtext="565656" />

    <class name="linkimage" cursor="hand"/>

    <class name="cls_edit" x-margin="2" y-margin="2" ncskin="skin_editnc"/>
  </style>

  <objattr>
    <button class="normalbtn"/>
    <check skin="btncheckbox"/>
    <radio skin="btnRadio"/>
    <imgbtn class="linkimage"/>
    <tabctrl tabskin="tabskin_default" crtext="000000" align="top" tabwidth="70" tabheight="38" tabspacing="0" tabpos="10" dotted="1"/>
    <divex sbskin="sb_common" sbwid="16"/>
    <edit transparent="1" x-margin="2" y-margin="2" ncskin="skin_editnc"/>
    <treectrl toggleskin="TreeToggle" checkskin="TreeCheckBox" critembg="FFFFFF" critemselbg="000088" critemtext="000000" critemseltext="FFFFFF" indent="17" itemmargin="4"/>
  </objattr>
</DuiEngine>